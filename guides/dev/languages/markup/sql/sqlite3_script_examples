

/* syntax
CREATE TABLE `Manga` (
	`ID`	INTEGER,
	`Name`	TEXT,
	`Overall score`	TEXT,
	`Current progress`	TEXT,
    `Author`	TEXT,
    `Genre`	TEXT,
    PRIMARY KEY(`ID`)
);


"name","type"
"ID","INT"
"Name(SK)","VARCHAR(29)"
"Overall Score","VARCHAR(3)"
"Current status", 
"Author","VARCHAR(8)"
"Genre","VARCHAR(22)"
