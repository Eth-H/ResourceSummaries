# merge tables
    //open db
        attach "mydb.sqlite" as db1;
    //merge databases
    attach './mydb2.db' as toMerge;           
    /* BEGIN; */
    insert into table1 select * from toMerge.table1; 
    COMMIT; 
    detach toMerge;

    //get columns
        SELECT name FROM PRAGMA_TABLE_INFO('tableName');
    insert into TableName.table1 select * from TableName2.table1;
