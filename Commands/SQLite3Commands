CREATE TABLE `Manga` (
	`ID`	INTEGER,
	`Name`	TEXT,
	`Overall score`	TEXT,
	`Current progress`	TEXT,
    `Author`	TEXT,
    `Genre`	TEXT,
    PRIMARY KEY(`ID`)
);


